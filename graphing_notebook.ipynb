    "import altair as alt\n",
    "from vega_datasets import data\n",
    "alt.renderers.enable('svg')\n",
    "\n",
    "\n",
    "cars = data.cars()\n",
    "# make the chart\n",
    "chart = alt.Chart(cars).mark_point().encode( # type: ignore\n",
    "    x='Horsepower',\n",
    "    y='Miles_per_Gallon',\n",
    "    color='Origin',\n",
    ").interactive()\n",
    "chart.show()"
   ]
  }
